		values (#{id,jdbcType=INTEGER}, #{dictTabname,jdbcType=VARCHAR},
		#{dictColname,jdbcType=VARCHAR},
		#{dictOption,jdbcType=VARCHAR}, #{dictText,jdbcType=VARCHAR}, #{dictSort,jdbcType=INTEGER},
		#{dictName,jdbcType=VARCHAR})
	</insert>
	<insert id="insertSelective" parameterType="com.lanou.entity.Dict">
