<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanou.mapper.DictMapper">
	<resultMap id="BaseResultMap" type="com.lanou.entity.Dict">
		<id column="id" jdbcType="INTEGER" property="id" />
		<result column="dict_tabname" jdbcType="VARCHAR" property="dictTabname" />
		<result column="dict_colname" jdbcType="VARCHAR" property="dictColname" />
		<result column="dict_option" jdbcType="VARCHAR" property="dictOption" />
		<result column="dict_text" jdbcType="VARCHAR" property="dictText" />
		<result column="dict_sort" jdbcType="INTEGER" property="dictSort" />
		<result column="dict_name" jdbcType="VARCHAR" property="dictName" />
	</resultMap>
	<resultMap extends="BaseResultMap" id="ResultMapWithBLOBs"
		type="com.lanou.entity.Dict">
		<result column="dict_sort" jdbcType="LONGVARCHAR" property="dictSort" />
	</resultMap>
	<sql id="Base_Column_List">
		id, dict_tabname, dict_colname, dict_option, dict_text, dict_sort,
		dict_name
	</sql>
	<sql id="Blob_Column_List">
		dict_sort
	</sql>
	<select id="selectByPrimaryKey" parameterType="java.lang.Integer"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from sys_dict
		where id = #{id,jdbcType=INTEGER}
	</select>
	<select id="selectAll" resultMap="ResultMapWithBLOBs">
		select
		<include refid="Base_Column_List" />
		,
		<include refid="Blob_Column_List" />
		from sys_dict
		where dict_tabname = 'scrap'
	</select>
	<!--遍历 -->
	<select id="getPageList" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from sys_dict
		<where>
			<if test="search != null and search !=''">
				INSTR(ifnull(dict_tabname,''),'${search}') > 0
			</if>
		</where>
		<if test="subSQL != null and subSQL !='' ">
			${subSQL}
		</if>
	</select>
	<select id="getTotalCount" resultType="java.lang.Integer">
		select
		count(1)
		from sys_dict
		<where>
			<if test="search != null and search !=''">
				INSTR(ifnull(dict_tabname,''),'${search}') > 0
			</if>
		</where>
	</select>

